resources:
- type: compute.v1.network
  name: eknetwork
  properties:
    autoCreateSubnetworks: false

- name: ek-firewall-rule
  type: compute.v1.firewall
  properties:
    network: $(ref.eknetwork.selfLink)
    sourceRanges: ["0.0.0.0/0"]
    allowed:
    - IPProtocol: TCP
      ports: ["80", "22", "9200", "5601"] 