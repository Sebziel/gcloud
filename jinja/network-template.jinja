resources:
- type: compute.v1.network
  name: training-deployment-network
  properties:
    autoCreateSubnetworks: false

- name: moj-homepage-firewall-rule
  type: compute.v1.firewall
  properties:
    network: $(ref.training-deployment-network.selfLink)
    sourceRanges: ["0.0.0.0/0"]
    allowed:
    - IPProtocol: TCP
      ports: ["80", "22"] 